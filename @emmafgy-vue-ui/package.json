{
  "main": "./lib/@emmafgy-vue.js",
  "name": "@emmafgy/vue-components",
  "description": "vue2 ui",
  "version": "1.0.61",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/fuguiyong/emmafgy-packages.git"
  },
  "author": "fuguiyong <760720981@qq.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "npm run build:theme && cross-env NODE_ENV=development webpack-dev-server --open --hot --config build/config/webpack.dev.js",
    "lib": "cross-env NODE_ENV=production webpack --progress --hide-modules --config build/config/webpack.lib.js && webpack --config build/config/webpack.components.js && npm run build:theme",
    "build:theme": "node build/bin/genCssFile && gulp build --gulpfile src/theme/gulpfile.js && cp-cli src/theme/lib lib/theme"
  },
  "dependencies": {
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cp-cli": "^1.0.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "url-loader": "^4.1.1",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^5.7.3"
  }
}
